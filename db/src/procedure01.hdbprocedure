PROCEDURE "procedure01"(  
    out outputTab TABLE(  
      ID NVARCHAR(36),  
      NAME NVARCHAR(5000)  
    )  
  )   
  LANGUAGE SQLSCRIPT   
  SQL SECURITY INVOKER   
  --DEFAULT SCHEMA <default_schema_name>  
  READS SQL DATA AS   
BEGIN   
  outputTab =  
    SELECT  "ID",  
            "NAME"  
    FROM "EDU03_AUTHORS";
END